{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
	      "useGeneratedStaticRoutes": true,
        "serverFiles": [
          "utils/helpers/seo.js",
          "api/swr.js",
          "config/index.js",
          "server/middleware/ipx.js"
        ]
      }
    }
  ],
  "trailingSlash": true
}
